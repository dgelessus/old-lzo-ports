const char *			T_PV
lzo_byte *			T_PV
lzo_bytep			T_PV
lzo_bytepp			T_PV
lzo_void *			T_PV
lzo_voidp			T_PV

lzo_int				T_IV
lzo_int32			T_IV
lzo_uint			T_UV
lzo_uint32			T_UV

#############################################################################
INPUT
T_UV
	$var = (unsigned long)SvUV($arg)
#############################################################################
OUTPUT
T_UV
	sv_setuv($arg, (IV)$var);
